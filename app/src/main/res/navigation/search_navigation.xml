<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/search_navigation"
    app:startDestination="@id/nav_search">

    <activity
        android:id="@+id/nav_search"
        android:name="com.bagel.noink.activity.SearchActivity"
        android:label="Search"
        tools:layout="@layout/activity_search">
    </activity>

    <fragment
        android:id="@+id/nav_search_result"
        android:name="com.bagel.noink.ui.history.SearchResultFragment"
        android:label="Search Result"
        tools:layout="@layout/fragment_search_result">
        <argument
            android:name="searchList"
            app:argType="java.util.ArrayList" />
    </fragment>

</navigation>
